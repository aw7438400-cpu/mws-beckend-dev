{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@0.8.0"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "continue": true
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}